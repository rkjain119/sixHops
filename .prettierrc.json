{
	"arrowParens": "always",
	"bracketSpacing": true,
	"importOrder": [
		"^react",
		"/types/",
		"/hooks/",
		"/store/",
		"/views/",
		"/components/",
		"/assets/",
		"^\\w",
		"css$",
		"^./"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"jsxSingleQuote": true,
	"printWidth": 100,
	"semi": false,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5"
}
